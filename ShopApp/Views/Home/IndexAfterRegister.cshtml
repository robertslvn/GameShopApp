@model HomeViewModel

<h4 class="regsuccessmessage">Thank you for registering. You may now log in with your new account!</h4>
@await Html.PartialAsync("Carousel")

<h3 class="featuredtitle">This Week's Featured Games</h3>


<div class="row">

    @foreach (var game in Model.GamesOfTheWeek)
    {
        @Html.Partial("GameOfTheWeekSummary", game)
    }

</div>